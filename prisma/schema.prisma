model Midia {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  name   String?
  src    String
  type   String?
  description String?
}
model Token {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  token      String   @unique
  expiresAt DateTime
}
model RefreshToken {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  token     String   @unique
  expiresAt DateTime
  createdAt DateTime @default(now())
}
